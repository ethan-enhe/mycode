{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "unbound"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": 
            {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command": "find"
        },
        {
            "command": 
            {
                "action": "quakeMode"
            },
            "keys": "win+`"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto"
            },
            "keys": "ctrl+shift+s"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            }
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": "closePane",
            "keys": "ctrl+shift+q"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            }
        }
    ],
    "alwaysOnTop": false,
    "alwaysShowTabs": false,
